{
  "name": "curso-de-webpack",
  "version": "1.0.0",
  "description": "Este curso te ense√±a a preparar tu entorno de desarrollo",
  "main": "index.js",
  "scripts": {
    "build": "webpack --entry ./index.js --output ./bundle.js --mode development",
    "build:local": "webpack --mode development",
    "build:external": "webpack --config ./external/webpack.config.js --mode development",
    "build:multi": "webpack --config ./multiples-entry-points/webpack.config.js --mode development",
    "build:css": "webpack --config ./css-style-loader/webpack.config.js --mode development",
    "build:extract:css": "webpack --config ./plugin-extract-css/webpack.config.js --mode development",
    "build:development": "webpack-dev-server --config ./webpack-dev-server/webpack.config.js",
    "build:babel": "webpack --config ./babel-loader/webpack.config.js --mode development",
    "build:react": "webpack --config ./react/webpack.config.js --mode development",
    "build:url": "webpack --config ./url-loader/webpack.config.js --mode development",
    "build:prepro": "webpack --config ./prepro/webpack.config.js --mode development",
    "build:preventDuplication": "webpack --config ./prevent-duplication/webpack.config.js --mode development",
    "build:dll": "webpack --config ./dll/webpack.dll.config.js --mode development",
    "build:preventDuplicationFast": "webpack --config ./dll/webpack.config.js --mode development",
    "build:dynamic:dll": "webpack --config ./dynamic-imports/webpack.dll.config.js --mode development",
    "build:dynamic": "webpack --config ./dynamic-imports/webpack.config.js --mode development"
  },
  "keywords": [
    "webpack"
  ],
  "author": "@LeonidasEsteban",
  "license": "MIT",
  "devDependencies": {
    "@babel/core": "7.4.5",
    "@babel/plugin-syntax-dynamic-import": "7.2.0",
    "@babel/plugin-transform-runtime": "7.4.4",
    "@babel/preset-env": "7.4.5",
    "@babel/preset-react": "7.0.0",
    "babel-loader": "8.0.6",
    "css-loader": "2.1.1",
    "file-loader": "1.1.11",
    "html-webpack-plugin": "3.2.0",
    "less": "3.9.0",
    "less-loader": "5.0.0",
    "mini-css-extract-plugin": "0.6.0",
    "node-sass": "4.13.1",
    "postcss-loader": "3.0.0",
    "postcss-nested": "4.1.2",
    "sass-loader": "7.1.0",
    "style-loader": "0.23.1",
    "stylus": "0.54.5",
    "stylus-loader": "3.0.2",
    "url-loader": "1.1.2",
    "webpack": "4.32.2",
    "webpack-cli": "3.3.2",
    "webpack-dev-server": "3.4.1"
  },
  "dependencies": {
    "react": "16.8.0",
    "@babel/runtime": "7.4.5",
    "react-dom": "16.8.0"
  }
}
